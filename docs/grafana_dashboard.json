{
  "dashboard": {
    "id": null,
    "title": "CodeBot Observability",
    "timezone": "browser",
    "schemaVersion": 36,
    "version": 1,
    "panels": [
      {
        "type": "gauge",
        "title": "Prediction Accuracy (%)",
        "id": 1,
        "gridPos": {"x": 0, "y": 0, "w": 8, "h": 8},
        "targets": [
          {"expr": "prediction_accuracy_percent", "legendFormat": "accuracy"}
        ],
        "options": {
          "reduceOptions": {"calcs": ["last"], "fields": "", "values": false},
          "showThresholdLabels": false,
          "showThresholdMarkers": true,
          "min": 0,
          "max": 100
        }
      },
      {
        "type": "timeseries",
        "title": "Prevented Incidents (rate/hour)",
        "id": 2,
        "gridPos": {"x": 8, "y": 0, "w": 16, "h": 8},
        "targets": [
          {"expr": "increase(prevented_incidents_total[1h])", "legendFormat": "prevented"}
        ]
      },
      {
        "type": "timeseries",
        "title": "Predicted vs Actual (rate/hour)",
        "id": 3,
        "gridPos": {"x": 0, "y": 8, "w": 24, "h": 8},
        "targets": [
          {"expr": "increase(predicted_incidents_total[1h])", "legendFormat": "predicted"},
          {"expr": "increase(actual_incidents_total[1h])", "legendFormat": "actual"}
        ]
      }
    ]
  }
}
