{
    "$schema": "vscode://schemas/color-theme",
    "name": "Tech Guide Dark+",
    "type": "dark",
    "colors": {
        "editor.background": "#0f0f23",
        "editor.foreground": "#d4d4d4",
        "editorCursor.foreground": "#0088cc",
        "editor.lineHighlightBackground": "#16213e",
        "editor.selectionBackground": "#3d5a8066",
        "editor.findMatchBackground": "#f39c1266",
        "editor.findMatchHighlightBackground": "#f39c1244",
        "editorLineNumber.foreground": "#3d5a80",
        "editorLineNumber.activeForeground": "#0088cc",
        "editorGutter.background": "#1a1a2e",
        "editorBracketMatch.border": "#0088cc",
        "editorBracketMatch.background": "#0088cc33",
        "editorIndentGuide.background": "#3d5a8044",
        "editorIndentGuide.activeBackground": "#0088cc",
        "sideBar.background": "#16213e",
        "sideBar.foreground": "#c3cee3",
        "sideBar.border": "#3d5a80",
        "sideBarTitle.foreground": "#eeeeee",
        "activityBar.background": "#1a1a2e",
        "activityBar.foreground": "#0088cc",
        "activityBar.border": "#3d5a80",
        "activityBarBadge.background": "#0088cc",
        "activityBarBadge.foreground": "#ffffff",
        "statusBar.background": "#0088cc",
        "statusBar.foreground": "#ffffff",
        "statusBar.border": "#005577",
        "titleBar.activeBackground": "#1a1a2e",
        "titleBar.activeForeground": "#eeeeee",
        "titleBar.inactiveBackground": "#0f0f23",
        "titleBar.inactiveForeground": "#c3cee3",
        "tab.activeBackground": "#16213e",
        "tab.activeForeground": "#eeeeee",
        "tab.inactiveBackground": "#1a1a2e",
        "tab.inactiveForeground": "#c3cee3",
        "tab.border": "#3d5a80",
        "tab.activeBorderTop": "#0088cc",
        "panel.background": "#16213e",
        "panel.border": "#3d5a80",
        "terminal.background": "#0f0f23",
        "terminal.foreground": "#d4d4d4",
        "terminal.ansiBlack": "#0f0f23",
        "terminal.ansiRed": "#f44747",
        "terminal.ansiGreen": "#6a9955",
        "terminal.ansiYellow": "#dcdcaa",
        "terminal.ansiBlue": "#569cd6",
        "terminal.ansiMagenta": "#c586c0",
        "terminal.ansiCyan": "#4ec9b0",
        "terminal.ansiWhite": "#d4d4d4",
        "terminal.ansiBrightBlack": "#808080",
        "terminal.ansiBrightRed": "#f44747",
        "terminal.ansiBrightGreen": "#6a9955",
        "terminal.ansiBrightYellow": "#dcdcaa",
        "terminal.ansiBrightBlue": "#569cd6",
        "terminal.ansiBrightMagenta": "#c586c0",
        "terminal.ansiBrightCyan": "#4ec9b0",
        "terminal.ansiBrightWhite": "#ffffff",
        "input.background": "#0f0f23",
        "input.foreground": "#eeeeee",
        "input.border": "#3d5a80",
        "input.placeholderForeground": "#3d5a80",
        "dropdown.background": "#16213e",
        "dropdown.foreground": "#eeeeee",
        "dropdown.border": "#3d5a80",
        "button.background": "#0088cc",
        "button.foreground": "#ffffff",
        "button.hoverBackground": "#0099dd",
        "focusBorder": "#0088cc",
        "textLink.foreground": "#569cd6",
        "textLink.activeForeground": "#6cb6ff",
        "badge.background": "#0088cc",
        "badge.foreground": "#ffffff",
        "scrollbar.shadow": "#00000066",
        "scrollbarSlider.background": "#3d5a8066",
        "scrollbarSlider.hoverBackground": "#3d5a8099",
        "scrollbarSlider.activeBackground": "#0088cc",
        "list.activeSelectionBackground": "#0088cc",
        "list.activeSelectionForeground": "#ffffff",
        "list.inactiveSelectionBackground": "#16213e",
        "list.hoverBackground": "#16213e",
        "list.focusBackground": "#0088cc44",
        "gitDecoration.addedResourceForeground": "#6a9955",
        "gitDecoration.modifiedResourceForeground": "#dcdcaa",
        "gitDecoration.deletedResourceForeground": "#f44747",
        "gitDecoration.untrackedResourceForeground": "#4ec9b0",
        "gitDecoration.ignoredResourceForeground": "#3d5a80",
        "editorError.foreground": "#f44747",
        "editorWarning.foreground": "#dcdcaa",
        "editorInfo.foreground": "#569cd6",
        "notificationsErrorIcon.foreground": "#f44747",
        "notificationsWarningIcon.foreground": "#dcdcaa",
        "notificationsInfoIcon.foreground": "#569cd6",
        "testing.iconPassed": "#6a9955",
        "testing.iconFailed": "#f44747",
        "editorWidget.background": "#1a1a2e",
        "editorHoverWidget.background": "#1a1a2e"
    },
    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#6A9955",
                "fontStyle": "italic"
            }
        },
        {
            "name": "String",
            "scope": [
                "string",
                "string.quoted",
                "string.quoted.single",
                "string.quoted.double",
                "string.quoted.triple"
            ],
            "settings": {
                "foreground": "#CE9178"
            }
        },
        {
            "name": "Template String",
            "scope": [
                "string.template",
                "string.interpolated"
            ],
            "settings": {
                "foreground": "#CE9178"
            }
        },
        {
            "name": "String Escape",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#D7BA7D"
            }
        },
        {
            "name": "Number",
            "scope": [
                "constant.numeric",
                "constant.numeric.integer",
                "constant.numeric.float",
                "constant.numeric.hex",
                "constant.numeric.octal",
                "constant.numeric.binary"
            ],
            "settings": {
                "foreground": "#B5CEA8"
            }
        },
        {
            "name": "Built-in constant",
            "scope": [
                "constant.language",
                "constant.language.boolean",
                "constant.language.null",
                "constant.language.undefined"
            ],
            "settings": {
                "foreground": "#569CD6"
            }
        },
        {
            "name": "Keyword",
            "scope": [
                "keyword",
                "keyword.other",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#569CD6"
            }
        },
        {
            "name": "Control Keywords (if, else, for, while, return)",
            "scope": [
                "keyword.control",
                "keyword.control.flow",
                "keyword.control.conditional",
                "keyword.control.loop",
                "keyword.control.return",
                "keyword.control.trycatch"
            ],
            "settings": {
                "foreground": "#C586C0"
            }
        },
        {
            "name": "Import/Export",
            "scope": [
                "keyword.control.import",
                "keyword.control.export",
                "keyword.control.from",
                "keyword.control.as"
            ],
            "settings": {
                "foreground": "#C586C0"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator",
                "keyword.operator.assignment",
                "keyword.operator.comparison",
                "keyword.operator.logical",
                "keyword.operator.arithmetic"
            ],
            "settings": {
                "foreground": "#D4D4D4"
            }
        },
        {
            "name": "Operator Keywords (new, typeof, instanceof)",
            "scope": [
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.typeof",
                "keyword.operator.instanceof"
            ],
            "settings": {
                "foreground": "#569CD6"
            }
        },
        {
            "name": "Function Declaration",
            "scope": [
                "entity.name.function",
                "entity.name.function.method",
                "meta.function-call",
                "meta.function-call.generic",
                "support.function",
                "support.function.builtin"
            ],
            "settings": {
                "foreground": "#DCDCAA"
            }
        },
        {
            "name": "Decorator / Annotation",
            "scope": [
                "entity.name.function.decorator",
                "meta.function.decorator",
                "meta.decorator",
                "punctuation.decorator"
            ],
            "settings": {
                "foreground": "#DCDCAA"
            }
        },
        {
            "name": "Class, Type, Interface",
            "scope": [
                "entity.name.type",
                "entity.name.type.class",
                "entity.name.type.interface",
                "entity.name.type.enum",
                "entity.name.class",
                "support.class",
                "support.type",
                "support.type.primitive"
            ],
            "settings": {
                "foreground": "#4EC9B0"
            }
        },
        {
            "name": "Inherited Class",
            "scope": [
                "entity.other.inherited-class"
            ],
            "settings": {
                "foreground": "#4EC9B0"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable",
                "variable.other",
                "variable.other.readwrite",
                "meta.definition.variable.name"
            ],
            "settings": {
                "foreground": "#9CDCFE"
            }
        },
        {
            "name": "Variable - Parameter",
            "scope": [
                "variable.parameter",
                "variable.parameter.function"
            ],
            "settings": {
                "foreground": "#9CDCFE"
            }
        },
        {
            "name": "Variable - Self/This",
            "scope": [
                "variable.language",
                "variable.language.this",
                "variable.language.self",
                "variable.language.super"
            ],
            "settings": {
                "foreground": "#569CD6"
            }
        },
        {
            "name": "Variable - Constant",
            "scope": [
                "variable.other.constant",
                "variable.other.enummember"
            ],
            "settings": {
                "foreground": "#4FC1FF"
            }
        },
        {
            "name": "Property",
            "scope": [
                "variable.other.property",
                "variable.other.object.property",
                "meta.object-literal.key",
                "support.type.property-name",
                "support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#9CDCFE"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation",
                "punctuation.separator",
                "punctuation.terminator",
                "punctuation.accessor",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#D4D4D4"
            }
        },
        {
            "name": "HTML/XML Tag",
            "scope": [
                "entity.name.tag",
                "entity.name.tag.html",
                "entity.name.tag.xml",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#569CD6"
            }
        },
        {
            "name": "HTML/XML Attribute",
            "scope": [
                "entity.other.attribute-name",
                "entity.other.attribute-name.html"
            ],
            "settings": {
                "foreground": "#9CDCFE"
            }
        },
        {
            "name": "CSS Property",
            "scope": [
                "support.type.property-name.css",
                "support.type.vendored.property-name.css"
            ],
            "settings": {
                "foreground": "#9CDCFE"
            }
        },
        {
            "name": "CSS Value",
            "scope": [
                "support.constant.property-value",
                "support.constant.font-name",
                "support.constant.color"
            ],
            "settings": {
                "foreground": "#CE9178"
            }
        },
        {
            "name": "CSS Selector",
            "scope": [
                "entity.other.attribute-name.class.css",
                "entity.other.attribute-name.id.css"
            ],
            "settings": {
                "foreground": "#D7BA7D"
            }
        },
        {
            "name": "Regex",
            "scope": [
                "string.regexp"
            ],
            "settings": {
                "foreground": "#D16969"
            }
        },
        {
            "name": "Regex - Character Class",
            "scope": [
                "constant.character.character-class.regexp",
                "constant.other.character-class.regexp"
            ],
            "settings": {
                "foreground": "#D16969"
            }
        },
        {
            "name": "Regex - Quantifier",
            "scope": [
                "keyword.operator.quantifier.regexp"
            ],
            "settings": {
                "foreground": "#D7BA7D"
            }
        },
        {
            "name": "Markdown - Heading",
            "scope": [
                "markup.heading",
                "entity.name.section.markdown"
            ],
            "settings": {
                "foreground": "#569CD6",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown - Bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "foreground": "#569CD6",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown - Italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "foreground": "#569CD6",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown - Link",
            "scope": [
                "markup.underline.link",
                "string.other.link"
            ],
            "settings": {
                "foreground": "#CE9178"
            }
        },
        {
            "name": "Markdown - Code",
            "scope": [
                "markup.raw",
                "markup.inline.raw"
            ],
            "settings": {
                "foreground": "#CE9178"
            }
        },
        {
            "name": "Markdown - Quote",
            "scope": [
                "markup.quote"
            ],
            "settings": {
                "foreground": "#6A9955",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Diff - Inserted",
            "scope": [
                "markup.inserted",
                "markup.inserted.diff"
            ],
            "settings": {
                "foreground": "#6A9955"
            }
        },
        {
            "name": "Diff - Deleted",
            "scope": [
                "markup.deleted",
                "markup.deleted.diff"
            ],
            "settings": {
                "foreground": "#F44747"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#F44747"
            }
        }
    ],
    "semanticTokenColors": {
        "newOperator": "#C586C0",
        "stringLiteral": "#CE9178",
        "customLiteral": "#DCDCAA",
        "numberLiteral": "#B5CEA8"
    }
}
